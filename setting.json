{
    "hosting": [
        {
            "name": "foo.com",
            "enable": true,
            "listen": "*:80",
            "origin": {
                "address": [
                    "kr.origin.com"
                ]
            },
            "aliases": [
                "foo.co.kr"
            ]
        },
        {
            "name": "bar.com",
            "enable": true,
            "listen": "*:80",
            "origin": {
                "address": [
                    "kr.origin.com"
                ]
            },
            "hook": {
                "enable": true
            },
            "functions": {
                "backend": {
                    "aws_s3": {
                        "meta": {
                            "enable": false
                        }
                    }
                },
                "contents": {
                    "dims": {
                        "meta": {
                            "enable": false,
                            "keyword": "dims"
                        }
                    },
                    "hyperdims": {
                        "meta": {
                            "enable": false
                        }
                    },
                    "pagespeed": {
                        "meta": {
                            "keyword": "pagespeed"
                        }
                    }
                },
                "network": {
                    "proxy": {
                        "meta": {
                            "enable": true
                        }
                    },
                    "mp3hls": {
                        "meta": {
                            "enable": false
                        }
                    },
                    "mp4hls": {
                        "meta": {
                            "enable": true
                        }
                    }
                }
            }
        }
    ]
}
